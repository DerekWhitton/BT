{
  "version": 1,
  "projects": {
    "administration-portal": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/administration-portal",
      "sourceRoot": "apps/administration-portal/src",
      "prefix": "bushtrade.web",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/administration-portal",
            "index": "apps/administration-portal/src/index.html",
            "main": "apps/administration-portal/src/main.ts",
            "polyfills": "apps/administration-portal/src/polyfills.ts",
            "tsConfig": "apps/administration-portal/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/administration-portal/src/favicon.ico",
              "apps/administration-portal/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/shared/assets",
                "output": "./assets"
              }
            ],
            "styles": ["apps/administration-portal/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/administration-portal/src/environments/environment.ts",
                  "with": "apps/administration-portal/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "administration-portal:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "administration-portal:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "administration-portal:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/administration-portal/tsconfig.app.json",
              "apps/administration-portal/tsconfig.spec.json",
              "apps/administration-portal/tsconfig.editor.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/administration-portal/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/administration-portal/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "administration-portal-e2e": {
      "root": "apps/administration-portal-e2e",
      "sourceRoot": "apps/administration-portal-e2e/src",
